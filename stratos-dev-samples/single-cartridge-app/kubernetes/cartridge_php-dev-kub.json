{
    "type": "php-kub",
    "provider": "apache",
    "category": "framework",
    "host": "php.stratos.org",
    "displayName": "php",
    "description": "php Cartridge",
    "version": "7",
    "multiTenant": "false",
    "loadBalancingIPType": "public",
    "portMapping": [
        {
            "name": "http-80",
            "protocol": "http",
            "port": "80",
            "proxyPort": "8281"
        }
    ],
    "deployment": {
        "baseDir": "/var/www"
    },
    "iaasProvider": [
        {
            "type": "kubernetes",
            "imageId": "ravihansa/php-dev:4.1.2",
            "networkInterfaces": [
            ],
            "property": [
            ]
        }
    ],
    "property": [
        {
            "name": "KUBERNETES_CONTAINER_CPU",
            "value": "0"
        },
        {
            "name": "KUBERNETES_CONTAINER_MEMORY",
            "value": "0"
        },
        {
            "name": "KUBERNETES_SERVICE_SESSION_AFFINITY",
            "value": "ClientIP"
        }
    ]
}
